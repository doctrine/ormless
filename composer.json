{
    "name": "doctrine/skeleton-mapper",
    "type": "library",
    "description": "The Doctrine SkeletonMapper is a skeleton object mapper where you are 100% responsible for implementing the guts of the persistence operations. This means you write plain old PHP code for the data repositories, object repositories, object hydrators and object persisters.",
    "keywords": ["database", "persistence", "object"],
    "homepage": "https://www.doctrine-project.org/projects/skeleton-mapper.html",
    "license": "MIT",
    "authors": [
        { "name": "Jonathan H. Wage", "email": "jonwage@gmail.com" }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/collections": "^1.8",
        "doctrine/instantiator": "^1.5",
        "doctrine/persistence": "^3.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "^11.0",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpunit/phpunit": "~9.5"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-0": {
            "Doctrine\\SkeletonMapper": "lib/",
            "Doctrine\\SkeletonMapper\\Tests": "tests/"
        }
    }
}
